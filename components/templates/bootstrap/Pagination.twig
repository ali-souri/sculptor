<nav id="{{id}}" aria-label="Page navigation">
  <ul class="pagination {{size_class|default('')}}">
    <li>
      <a href="{{prev_link|default('#')}}" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
      </a>
    </li>
    {% for link in links %}
      <li {% if link.active is defined %} {% if link.active %} class="active" {% endif %}{% endif %}><a href="{{link.href|default('#')}}">{{link.number|default(loop.index)}} {% if link.active is defined %} {% if link.active %} <span class="sr-only">(current)</span> {% endif %}{% endif %}></a></li>
    {% endfor %}
    <li>
      <a href="{{next_link|default('#')}}" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
      </a>
    </li>
  </ul>
</nav>